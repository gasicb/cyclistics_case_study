CREATE TABLE cyclistic_data(
ride_id	nvarchar(50) PRIMARY KEY,
rideable_type	nvarchar(50),
started_at	smalldatetime,
ended_at	smalldatetime,
start_station_name	nvarchar(50),
start_station_id	int,
end_station_name	nvarchar(50),
end_station_id	int,
start_lat	float,
start_lng	float,
end_lat	float,
end_lng	float,
member_casual	nvarchar(50))


BULK INSERT cyclistic_data
FROM 'C:\Users\boris\OneDrive\Desktop\Data Analytics\Case study\Bike data\last_12months\202008-divvy-tripdata.csv'
WITH (
	FORMAT = 'CSV',
	FIRSTROW = 1 ,
	FIELDTERMINATOR = ',', 
	ROWTERMINATOR = '\n'
)
